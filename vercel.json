{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "env": {
        "PORT": "3333"
      },
      "buildCommand": "npm run dev"
    },
    {
      "src": "server.json",
      "use": "@vercel/node-server",
      "env": {
        "PORT": "5000"
      },
      "buildCommand": "npm run dev:server"
    }
  ]
}
